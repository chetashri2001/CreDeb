<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/dark_blue"
        android:backgroundTint="@color/dark_blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <!--            <ImageView-->
            <!--                android:id="@+id/menu"-->
            <!--                android:layout_width="24dp"-->
            <!--                android:layout_height="24dp"-->
            <!--                android:src="@drawable/ic_baseline_menu_24"-->
            <!--                app:tint="@color/white" />-->
            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="16dp"
                android:text="CreDeb"
                android:textColor="@color/white"
                android:textSize="24dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="89dp"
                tools:layout_editor_absoluteY="16dp" />

        </LinearLayout>

    </androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            android:padding="10dp">
            <TextView
                android:id="@+id/editText11"
                android:layout_marginTop="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:fontFamily="@font/bold"
                android:text="ABOUT"
                android:gravity="center"
                android:layout_marginBottom="15dp"
                android:textSize="18dp"/>
            <TextView
                android:id="@+id/editText1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:fontFamily="@font/regular"
                android:paddingRight="4dp"
                android:paddingLeft="4dp"
                android:text="@string/large_text"
                android:textSize="18dp"/>

            <TextView
                android:id="@+id/editText2"
                android:layout_marginTop="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:fontFamily="@font/bold"
                android:text="DEVELOPERS"
                android:gravity="center"
                android:textSize="18dp"/>

            <TextView
                android:id="@+id/editText3"
                android:layout_marginTop="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:gravity="center"
                android:fontFamily="@font/bold"
                android:layout_marginBottom="14dp"
                android:text="2019130027"
                android:textSize="18dp"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:src="@drawable/manvita1"
                />
            <TextView
                android:id="@+id/editText4"
                android:layout_marginTop="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:gravity="center"
                android:fontFamily="@font/regular"
                android:text="Manvita Joshi"
                android:layout_marginBottom="20dp"
                android:textSize="18dp"/>

            <TextView
                android:id="@+id/editText5"
                android:layout_marginTop="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:gravity="center"
                android:fontFamily="@font/bold"
                android:layout_marginBottom="14dp"
                android:text="2019130031"
                android:textSize="18dp"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:src="@drawable/traividya2"
                />
            <TextView
                android:id="@+id/editText6"
                android:layout_marginTop="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:gravity="center"
                android:fontFamily="@font/regular"
                android:text="Traividya Korgaonkar"
                android:layout_marginBottom="20dp"
                android:textSize="18dp"/>

            <TextView
                android:id="@+id/editText7"
                android:layout_marginTop="30dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:gravity="center"
                android:fontFamily="@font/bold"
                android:layout_marginBottom="14dp"
                android:text="2019130035"
                android:textSize="18dp"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:src="@drawable/chetashri2"
                />
            <TextView
                android:id="@+id/editText8"
                android:layout_marginTop="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:gravity="center"
                android:fontFamily="@font/regular"
                android:text="Chetashri Mahajan"
                android:layout_marginBottom="20dp"
                android:textSize="18dp"/>

        </LinearLayout>
    </ScrollView>



</LinearLayout>


